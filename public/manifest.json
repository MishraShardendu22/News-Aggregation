{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "News-Aggregation",
  "description": "A basic example extension with only required keys",
  "icons": { 
    "128": "icon.png", 
    "64": "icon.png", 
    "32": "icon.png",
    "16": "icon.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
